[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Command Processor]
"AutoRun"="C:\\local\\bin\\_autorun.cmd"pwd=cd $*

cd=pushd $*

cd~=cd C:\home\hichens
 $*
ll=dir /a$*

ls=dir /b $*
